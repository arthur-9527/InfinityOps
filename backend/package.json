{
  "name": "infinityops-backend",
  "version": "1.0.0",
  "description": "InfinityOps Backend Service",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "pnpm nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:ai": "ts-node src/scripts/test-ai.ts",
    "test:ai:factory": "ts-node src/scripts/test-ai.ts factory",
    "test:ai:models": "ts-node src/scripts/test-ai.ts models",
    "test:ai:completion": "ts-node src/scripts/test-ai.ts completion",
    "test:ai:ollama": "ts-node src/scripts/test-ai.ts ollama",
    "test:command": "ts-node src/scripts/test-command-analysis.ts",
    "print:env": "ts-node -e 'require(\"./src/config/env-example\").printExampleConfig()'",
    "lint": "pnpm eslint . --ext .ts",
    "format": "pnpm prettier --write \"src/**/*.ts\"",
    "test:ssh": "ts-node src/scripts/test-ssh.ts",
    "test:ssh-direct": "ts-node src/scripts/test-ssh-direct.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/websockets": "^11.1.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "node-pty": "^1.0.0",
    "socket.io": "^4.8.1",
    "ssh2": "^1.16.0",
    "winston": "^3.17.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.32",
    "@types/ssh2": "^1.15.5",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
